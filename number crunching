import java.util.*;
class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
 System.out.println("Enter  number:");
      int n=sc.nextInt();
      
      int sum=0;
      for( int i=0; i<n; i++) {
      sum = i +(i+1);
      System.out.println(sum);
    }
}
}

Enter  number:
5
1
3
5
7
9
