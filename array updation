class Main {
    public static void main(String[] args) {
        String[] arr={"hii","welcome","apple","cat","words"};
           
        for(  int i=0; i<arr.length;i++){
            if(arr[i]=="apple"){
                arr[i]="vowels";
                 System.out.println("updated value:"+arr[i]);
            }
         
          System.out.println(arr[i]);
    }
    }
}
