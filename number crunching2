import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner data = new Scanner(System.in);

System.out.println("enter number:");
int n=data.nextInt();

int sum=0;
for (int i = 0; i < n; i++) {
    sum = sum + (i + 1);
    System.out.println(sum);
}
        
}
}


enter number:
3
1
3
6
